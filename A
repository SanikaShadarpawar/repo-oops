package Github;

class A {
    void msg() {
        System.out.println("Class A");
    }
}

class B extends A {
    void msg() {
        System.out.println("Class B");
    }
}

class C extends B {
    void msg() {
        System.out.println("Class C");
    }

    public static void main(String[] args) {
        new C().msg();
    }
}
