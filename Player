package Github;

class Player {
    String name;

    Player() {
        this.name = "Unknown";
    }

    Player(String name) {
        this.name = name;
    }

    void display() {
        System.out.println("Player: " + name);
    }

    public static void main(String[] args) {
        Player p1 = new Player();
        Player p2 = new Player("virat");
        p1.display();
        p2.display();
    }
}
