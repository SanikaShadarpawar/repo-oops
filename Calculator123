package Github;

class Calculator123 {
    int add(int a, int b) {
        return a + b;
    }
    double add(double a, double b) {
        return a + b;
    }
    public static void main(String[] args) {
        Calculator123 c = new Calculator123();
        System.out.println(c.add(2, 3));
        System.out.println(c.add(2.5, 3.5));
    }
}
